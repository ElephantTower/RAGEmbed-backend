generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["postgresqlExtensions"]
  // output   = "../../generated/prisma"
}

generator json {
  provider = "prisma-json-types-generator"
}

datasource db {
  provider   = "postgresql"
  url        = env("DATABASE_URL")
  extensions = [vector]
}
